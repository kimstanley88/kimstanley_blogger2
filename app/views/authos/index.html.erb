<p id="notice"><%= notice %></p>

<h1>Listing Authos</h1>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Email</th>
      <th>Password</th>
      <th>Password confirmation</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @authos.each do |autho| %>
      <tr>
        <td><%= autho.username %></td>
        <td><%= autho.email %></td>
        <td><%= autho.password %></td>
        <td><%= autho.password_confirmation %></td>
        <td><%= link_to 'Show', autho %></td>
        <td><%= link_to 'Edit', edit_autho_path(autho) %></td>
        <td><%= link_to 'Destroy', autho, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Autho', new_autho_path %>
